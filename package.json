  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "crypto-js": "^3.1.9-1",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "nodemon": "^2.0.1",
